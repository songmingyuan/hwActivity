       <extensionElements>
        <activiti:executionListener event="start" class="com.huiway.activiti.common.listener.ApplyExecutionListener"></activiti:executionListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask12" name="开发组长" activiti:candidateUsers="${kfzz}"></userTask>
    <sequenceFlow id="flow17" name="提交开发组长" sourceRef="usertask10" targetRef="usertask12"></sequenceFlow>
    <sequenceFlow id="flow18" name="提交开发组长" sourceRef="usertask11" targetRef="usertask12"></sequenceFlow>
    <sequenceFlow id="flow19" name="提交人事部/系统部" sourceRef="usertask12" targetRef="parallelgateway1"></sequenceFlow>
    <userTask id="usertask13" name="产品部" activiti:candidateUsers="${cpb}"></userTask>
    <sequenceFlow id="flow20" name="提交产品部" sourceRef="usertask4" targetRef="usertask13"></sequenceFlow>
    <parallelGateway id="parallelgateway2" name="路由选择"></parallelGateway>
    <sequenceFlow id="flow21" name="提交综合部" sourceRef="parallelgateway2" targetRef="usertask4"></sequenceFlow>
    <sequenceFlow id="flow22" sourceRef="usertask2" targetRef="parallelgateway2"></sequenceFlow>
    <sequenceFlow id="flow23" sourceRef="usertask3" targetRef="parallelgateway2"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="路由选择"></exclusiveGateway>
    <sequenceFlow id="flow24" sourceRef="usertask7" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow25" name="小于等于3" sourceRef="exclusivegateway2" targetRef="usertask10">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${num<=3}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow26" name="大于3" sourceRef="exclusivegateway2" targetRef="usertask11">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${num>3}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask14" name="副组长" activiti:candidateUsers="${fzz}"></userTask>
    <sequenceFlow id="flow27" name="提交" sourceRef="usertask1" targetRef="usertask14"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="路由选择"></exclusiveGateway>
    <sequenceFlow id="flow28" sourceRef="usertask6" targetRef="exclusivegateway3"></sequenceFlow>
    <sequenceFlow id="flow29" name="同意" sourceRef="exclusivegateway3" targetRef="usertask7">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper=='y'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow30" name="不同意" sourceRef="exclusivegateway3" targetRef="usertask14">
      <extensionElements>
        <activiti:executionListener event="take" class="com.huiway.activiti.common.listener.ApplyNoExecutionListener"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper=='n'}]]></conditionExpression>
    </sequenceFlow>
