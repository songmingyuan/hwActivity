  <process id="userApply" name="用户申请" isExecutable="true">
    <startEvent id="startevent1" name="Start" activiti:initiator="applyUser"></startEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="usertask1" name="文员" activiti:assignee="${applyUser}"></userTask>
    <parallelGateway id="parallelgateway1" name="路由选择"></parallelGateway>
    <userTask id="usertask2" name="人事部" activiti:candidateUsers="${rsb}">
       <extensionElements>
        <activiti:executionListener event="start" class="com.huiway.activiti.common.listener.ApplyExecutionListener"></activiti:executionListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask3" name="系统部" activiti:candidateUsers="${xtb}">
       <extensionElements>
        <activiti:executionListener event="start" class="com.huiway.activiti.common.listener.ApplyExecutionListener"></activiti:executionListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask4" name="综合部" activiti:candidateUsers="${zhb}">
       <extensionElements>
        <activiti:executionListener event="start" class="com.huiway.activiti.common.listener.ApplyExecutionListener"></activiti:executionListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" name="提交" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow3" name="提交系统部" sourceRef="parallelgateway1" targetRef="usertask3"></sequenceFlow>
    <sequenceFlow id="flow4" name="提交人事部" sourceRef="parallelgateway1" targetRef="usertask2"></sequenceFlow>
    <sequenceFlow id="flow7" name="结束" sourceRef="usertask13" targetRef="endevent1"></sequenceFlow>
    <userTask id="usertask5" name="会签小组" activiti:assignee="${xzPerson}">
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${xzPersonsList}" activiti:elementVariable="xzPerson">
        <completionCondition>${nrOfCompletedInstances/nrOfInstances &gt;= 1}</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <sequenceFlow id="flow8" name="提交" sourceRef="usertask14" targetRef="usertask5"></sequenceFlow>
    <userTask id="usertask6" name="招聘" activiti:candidateUsers="${zp}"></userTask>
    <sequenceFlow id="flow9" name="提交" sourceRef="usertask5" targetRef="usertask6"></sequenceFlow>
    <userTask id="usertask7" name="组长" activiti:candidateUsers="${zz}">
       <extensionElements>
        <activiti:executionListener event="start" class="com.huiway.activiti.common.listener.ApplyExecutionListener"></activiti:executionListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask10" name="产品经理"  activiti:candidateUsers="${cpjl}">
      <extensionElements>
        <activiti:executionListener event="start" class="com.huiway.activiti.common.listener.ApplyExecutionListener"></activiti:executionListener>
      </extensionElements>
    </userTask>
