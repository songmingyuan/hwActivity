        <activiti:executionListener event="start" class="com.huiway.activiti.common.listener.MyExecutionListener"></activiti:executionListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask5" name="人事" activiti:candidateUsers="${rsUser}"></userTask>
    <exclusiveGateway id="exclusivegateway1" name="路由选择"></exclusiveGateway>
    <sequenceFlow id="flow5" name="小于等于3天" sourceRef="exclusivegateway1" targetRef="usertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${days<=3}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" name="大于3天" sourceRef="exclusivegateway1" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${days>3}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask6" name="员工确认" activiti:assignee="applyUser"></userTask>
    <sequenceFlow id="flow7" sourceRef="usertask4" targetRef="usertask5"></sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="usertask3" targetRef="usertask5"></sequenceFlow>
    <sequenceFlow id="flow9" name="提交" sourceRef="usertask5" targetRef="usertask6"></sequenceFlow>
    <sequenceFlow id="flow10" name="结束" sourceRef="usertask6" targetRef="endevent1"></sequenceFlow>
